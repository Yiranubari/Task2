{
    # Tell Railway we're using PHP
    order frankenphp before file_server
}

# Your domain
:8080 {
    # Set the root to the /app directory
    root * /app

    # Enable PHP
    frankenphp

    # Rewrite all requests that aren't files to index.php
    @notFile {
        not file
    }
    rewrite @notFile /index.php?route={path}
}